{
  "name": "spigot-buyer-list",
  "version": "1.1.1",
  "description": "A NodeJS package that allows you to fetch the buyers list on Spigot for a premium plugin",
  "main": "dist/index.js",
  "repository": "git@github.com:diogotcorreia/spigot-buyer-list.git",
  "author": "Diogo Correia <me@diogotc.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "spigot",
    "minecraft"
  ],
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "babel-preset-latest-node": "^4.1.0",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec babel-node",
    "build": "babel src -d dist",
    "format": "prettier --write src/**/*.{js,jsx}",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "dependencies": {
    "cloudscraper": "^4.6.0",
    "request": "^2.88.2",
    "speakeasy": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
